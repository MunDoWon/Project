@model IEnumerable<LastPatient.Controllers.VisitPatients>

@{
    ViewData["Title"] = "Visit";
}
<h1>면회신청현황</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VisitName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VisitPatient)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VisitHp)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                    @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.VisitName)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.VisitPatient)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.VisitHp)
            </td>
                <td>
                    @Html.DisplayFor(modelItem => item.VisitOX)
                </td>
            <td>
                <a asp-action="Check" asp-route-id="@item.Id">면회승인</a> |
                    <a asp-action="Del" asp-route-id="@item.Id">삭제</a>
            </td>
        </tr>
}
    </tbody>
</table>
<br/>
<a class="btn btn-primary" id="btn_back" asp-action="Medicine">뒤로가기</a>
